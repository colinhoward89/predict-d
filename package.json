{
  "name": "predict-d",
  "version": "1.0.0",
  "description": "Predict football matches against your friends",
  "main": "index.js",
  "scripts": {
    "install": "cd client && npm i . && cd ../server && npm i .",
    "server": "npx ts-node server/src/server.ts",
    "client": "cd client && npm start && cd .. ",
    "start:server-client": "concurrently \"npm run server\" \"npm run client \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colinhoward89/predict-d"
  },
  "dependencies": {
    "concurrently": "^8.2.0"
  }
}
